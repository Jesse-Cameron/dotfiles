[user]
	name = Jesse-Cameron
	email = jessec.cameron@gmail.com
[core]
	excludesfile = ~/.gitignore_global
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
[pull]
	ff = only
	rebase = true
[rebase]
	autosquash = true
[alias]
	default-branch = "!git symbolic-ref refs/remotes/origin/HEAD | cut -f4 -d/"
[gpg]
	format = ssh
[commit]
	gpgsign = true
[tag]
	gpgsign = true
[init]
	defaultBranch = main
